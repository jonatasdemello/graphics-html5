10 REM LANCAMENTO OBLIQUO
20 TEXT: PRINT "DIGITE VELOCIDADE V, INCLINACAO I (1-90)"
30 INPUT V,I
40 HGR : HCOLOR=6 : MX=279 : MY=159
50 PI=3.1415:G=9.8:T=0
60 I=I*2*PI/360
70 X=V*COS(I)*T : Y=V*SIN(I)*T-G*T*T/2
75 PRINT X ;", ";Y
80 IF X>MX OR Y>MY OR Y<0 THEN 120
90 HPLOT X,MY-Y
100 T=T+0.1
110 GOTO 70
120 END
