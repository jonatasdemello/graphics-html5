0 REM ** RENUMBER ONLY **
5 HGR2 : HCOLOR=1 : GOTO 100
10 R=SQR(X*X+Y*Y)*1.5 : IF R=0 THEN F=90: GOTO 30
20 F=ABS(90*SIN(R)/R)
30 A=INT(10*X+160-5*Y)
40 B=INT(5*Y+2.5*X+93)
60 RETURN
100 FOR Y=-10 TO 10 STEP 0.2
110 FOR X=-10 TO 10 STEP 0.3
120 GOSUB 20
130 IF A < 279 THEN HPLOT A, B-F
140 NEXT X
150 NEXT Y
