5 HGR2 : HCOLOR=1
100 FOR Y=-10 TO 10 STEP 0.2
110 FOR X=-10 TO 10 STEP 0.3
120 R=SQR(X*X+Y*Y)*1.5
130 IF R=0 THEN F=90 : GOTO 130
140 F=ABS(90*SIN(R)/R)
150 A=INT(10*X+160-5*Y)
160 B=INT(5*Y+2.5*X+93)
170 IF A < 279 THEN HPLOT A, B-F
180 NEXT X
190 NEXT Y


5  HGR : HCOLOR=1
10 FOR Y = -10 TO 10 STEP .2
15  Y2 = Y * Y
16  Y5 = 5 * Y
20  FOR X = -10 TO 10 STEP .3
30   R = SQR(X * X + Y2) * 1.5
35   IF R = 0 THEN F = 90 : GOTO 50
40   F = ABS(90 * SIN(R) / R)
50   A = INT(10*X + 160 - Y5)
55   IF A > 278 THEN GOTO 70
60   B = INT(Y5 + 2.5*X + 93) - F
61   C = INT(B - F)
64   if A>279 OR C<0 then GOTO 70
65   HPLOT A,C
70  NEXT X
80 NEXT Y
90 END

